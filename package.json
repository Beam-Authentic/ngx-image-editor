{
  "name": "ngx-image-editor",
  "version": "1.0.0-semantically-released",
  "description": "Angular 5 Image Editor",
  "scripts": {
    "clean": "rm -rf dist ",
    "prepublish": "npm run clean",
    "build": "ng-packagr -p ng-package.json",
    "semantic-release": "semantic-release pre && npm run build && npm publish dist/ && semantic-release post"
  },
  "main": "./bundles/ng.umd.js",
  "module": "./ngx-image-editor.es5.js",
  "typings": "./ngx-image-editor.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Centroida/ngx-image-editor"
  },
  "keywords": [
    "Angular 2",
    "Angular 4",
    "Angular 5",
    "Angular",
    "ng",
    "Editor",
    "Image Editor",
    "Image Preview",
    "Cropper",
    "Uploader"
  ],
  "author": "Centroida",
  "license": "MIT",
  "homepage": "https://github.com/Centroida/ngx-image-editor",
  "devDependencies": {
    "@angular/animations": "4.3.6",
    "@angular/cdk": "2.0.0-beta.11",
    "@angular/common": "4.3.6",
    "@angular/compiler": "4.3.6",
    "@angular/compiler-cli": "4.4.3",
    "@angular/core": "4.3.6",
    "@angular/flex-layout": "2.0.0-beta.12",
    "@angular/forms": "4.3.6",
    "@angular/http": "4.3.6",
    "@angular/material": "2.0.0-beta.11",
    "@angular/platform-browser": "4.3.6",
    "@angular/platform-browser-dynamic": "4.3.6",
    "@types/core-js": "^0.9.43",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "2.4.0",
    "zone.js": "^0.8.4",
    "semantic-release": "^8.2.0"
  },
  "dependencies": {
    "@types/cropperjs": "0.0.26",
    "cropperjs": "^1.0.0"
  },
  "release": {
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {
          "type": "docs",
          "scope": "README",
          "release": "patch"
        },
        {
          "type": "refactor",
          "scope": "/src",
          "release": "minor"
        },
        {
          "type": "refactor",
          "release": "patch"
        }
      ]
    }
  }
}
