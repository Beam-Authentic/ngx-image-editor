{
  "name": "ngx-image-editor",
  "version": "1.0.0-semantically-released",
  "description": "Angular 5 Image Editor",
  "scripts": {
    "test": "echo \"Test success\"",
    "clean": "rm -rf dist ",
    "build": "ng-packagr -p ng-package.json",
    "semantic-release": "npm run clean  && npm run build && semantic-release pre && npm publish dist.tgz && semantic-release post"
  },
  "main": "./bundles/ng.umd.js",
  "module": "./ngx-image-editor.es5.js",
  "typings": "./ngx-image-editor.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Centroida/ngx-image-editor"
  },
  "keywords": [
    "Angular 2",
    "Angular 4",
    "Angular 5",
    "Angular 6",
    "Angular",
    "ng",
    "Editor",
    "Image Editor",
    "Image Preview",
    "Cropper"
  ],
  "author": "Hristo Georgiev",
  "license": "MIT",
  "homepage": "https://github.com/Centroida/ngx-image-editor",
  "devDependencies": {
    "@angular/animations": "6.0.0",
    "@angular/cdk": "^5.2.4",
    "@angular/cli": "6.0.0",
    "@angular/common": "6.0.0",
    "@angular/compiler": "6.0.1",
    "@angular/compiler-cli": "6.0.1",
    "@angular/core": "6.0.0",
    "@angular/flex-layout": "^5.0.0-beta.14",
    "@angular/forms": "6.0.0",
    "@angular/http": "6.0.0",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "6.0.0",
    "@angular/platform-browser-dynamic": "6.0.0",
    "@types/core-js": "0.9.46",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.2.1",
    "core-js": "^2.5.3",
    "ng-packagr": "^2.4.4",
    "rxjs": "^6.1.0",
    "semantic-release": "^8.2.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "2.7.2",
    "zone.js": "~0.8.26"
  },
  "peerDependencies": {
    "@types/cropperjs": "^1.1.3",
    "cropperjs": "^1.0.0",
    "ng-packagr": "^2.4.4",
    "rxjs-compat": "^6.0.0-rc.0",
    "tsickle": "^0.28.0",
    "tslib": "^1.9.0"
  },
  "release": {
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {
          "type": "docs",
          "scope": "README.md",
          "release": "patch"
        },
        {
          "type": "refactor",
          "scope": "/src",
          "release": "minor"
        },
        {
          "type": "refactor",
          "release": "patch"
        }
      ]
    }
  }
}
